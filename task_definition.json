{
    
    "executionRoleArn": "arn:aws:iam::301013669305:role/ecsTaskExecutionRole",
    "containerDefinitions": [
      {
        
        "logConfiguration": {
          "logDriver": "awslogs",
          
          "options": {
            "awslogs-group": "/ecs/express-task-defination",
            "awslogs-region": "eu-west-2",
            "awslogs-stream-prefix": "ecs"
          }
        },
        
        "portMappings": [
          {
            "hostPort": 3000,
            "protocol": "tcp",
            "containerPort": 3000
          }
        ],
        
        
        "cpu": 0,
